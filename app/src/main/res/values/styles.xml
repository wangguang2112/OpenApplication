<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="popanim_down">
        <item name="android:windowEnterAnimation">@anim/pop_down</item>
        <item name="android:windowExitAnimation">@anim/pop_up</item>
    </style>
    <style name="popanim_down2">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_right</item>
        <item name="android:windowExitAnimation">@anim/slide_out_to_left</item>
    </style>
</resources>
